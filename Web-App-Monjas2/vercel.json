{
  "version": 2,
  "cleanUrls": false,
  "rewrites": [
    {
      "source": "/landing",
      "destination": "/landing/index.html"
    },
    {
      "source": "/splat/:figure",
      "destination": "/index.html?type=splat&figure=:figure"
    },
    {
      "source": "/glb/:model",
      "destination": "/index.html?type=glb&model=:model"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "model"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "figure"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "type"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/landing/index.html"
    },
    {
      "source": "/models/:file",
      "destination": "/api/protect?f=/models/:file",
      "missing": [
        {
          "type": "query",
          "key": "ok"
        }
      ]
    },
    {
      "source": "/assets/audio/:file",
      "destination": "/api/protect?f=/assets/audio/:file",
      "missing": [
        {
          "type": "query",
          "key": "ok"
        }
      ]
    },
    {
      "source": "/:slug",
      "destination": "/index.html?type=glb&model=:slug"
    }
  ],
  "headers": [
    {
      "source": "/models/(.*)\\.usdz",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/vnd.usdz+zip"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex, noimageindex, noarchive"
        }
      ]
    },
    {
      "source": "/models/(.*)\\.glb",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/gltf-binary"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex, noimageindex, noarchive"
        }
      ]
    },
    {
      "source": "/assets/hdr/(.*)\\.hdr",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/vnd.radiance"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}